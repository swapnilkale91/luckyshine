{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
        "new-parens": true,
        "newline-before-return": true,
        "no-trailing-whitespace": true,
        "eofline": true,
        "import-spacing": true,
        "no-switch-case-fall-through": true,
        "no-string-literal": true,
        "no-default-export": true,
        "triple-equals": true,
        "prefer-method-signature": true,
        "no-arg": true,
        "no-bitwise": true,
        "no-conditional-assignment": true,
        "no-construct": true,
        "no-debugger": true,
        "no-duplicate-super": true,
        "no-duplicate-variable": true,
        "no-empty": true,
        "no-eval": true,
        "no-reference": true,
        "no-shadowed-variable": true,
        "no-string-throw": true,
        "no-unnecessary-initializer": true,
        "no-unsafe-finally": true,
        "no-unused-expression": true,
        "no-var-keyword": true,
        "object-literal-shorthand": true,
        "object-literal-sort-keys": false,
        "prefer-const": true,
        "prefer-for-of": true,
        "radix": true,
        "switch-default": true,
        "use-isnan": true,
        "arrow-return-shorthand": true,
        "class-name": true,
        "file-name-casing": [true, "kebab-case"],
        "align": [
            true,
            "parameters",
            "statements"
        ],
        "arrow-parens": [
            true,
            "ban-single-arg-parens"
        ],
        "indent": [
            true,
            "spaces"
        ],
        "linebreak-style": [
            false,
            "CRLF"
        ],
        "max-line-length": [
            true,
            255
        ],
        "no-consecutive-blank-lines": [
            true,
            1
        ],
        "quotemark": [
            true,
            "single",
            "avoid-escape"
        ],
        "trailing-comma": [
            true,
            {
                "multiline": "never",
                "singleline": "never"
            }
        ],
        "whitespace": [
            true,
            "check-branch",
            "check-decl",
            "check-operator",
            "check-separator",
            "check-type",
            "check-typecast"
        ],
        "completed-docs": [
            false,
            "classes",
            "functions",
            "methods",
            "properties",
            "enums",
            "interfaces",
            "types"
        ],
        "cyclomatic-complexity": [
            true,
            9
        ],
        "max-file-line-count": [
            true,
            512
        ],
        "no-console": [
            true,
            "log",
            "trace"
        ],
        "no-import-side-effect": [
            true,
            {
                "ignore-module": "(.html|.css)$"
            }
        ],
        "no-invalid-this": [
            true,
            "check-function-in-method"
        ],
        "ordered-imports": [
            true,
            {
                "import-sources-order": "case-insensitive",
                "module-source-path": "full",
                "named-imports-order": "case-insensitive"
            }
        ],
        "comment-format": [
            true,
            "check-space",
            "check-uppercase",
            {
                "ignore-words": [
                    "TODO",
                    "HACK"
                ]
            }
        ],
        "curly": true,
        "forin": true,
        "label-position": true,
        "max-classes-per-file": [
            true,
            1
        ],
        "member-ordering": [
            true,
            "statics-first"
        ],
        "no-magic-numbers": [
            true,
            0,
            1
        ],
        "object-literal-key-quotes": [
            true,
            "always"
        ],
        "one-line": [
            true,
            "check-catch",
            "check-else",
            "check-finally",
            "check-open-brace",
            "check-whitespace"
        ],
        "one-variable-per-declaration": [
            true,
            "ignore-for-loop"
        ],
        "only-arrow-functions": [
            true,
            "allow-declarations",
            "allow-named-functions"
        ],
        "semicolon": [
            true,
            "always"
        ],
        "space-before-function-paren": [
            true,
            {
                "anonymous": "never",
                "asyncArrow": "always",
                "constructor": "never",
                "method": "never",
                "named": "never"
            }
        ],
        "variable-name": [
            true,
            "ban-keywords",
            "check-format"
        ]
    },
